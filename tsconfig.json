    "compilerOptions": {
        "baseUrl": ".",
        "target": "ES2015",
        "module": "UMD",
        "sourceMap": true,
        "outDir": "dist",
        "skipLibCheck": true,
        "listEmittedFiles": true,
        "noEmitOnError": true,
        "moduleResolution": "Node",
