{
    "presets": [
        [
            "@babel/preset-env",
            {
                "useBuiltIns": "usage",
                "corejs": {
                    "version": 3,
                    "proposals": true
                }
            }
        ],
        "@babel/preset-typescript"
    ],
    "plugins": [
        [
            "module-resolver",
            {
                "root": ["."],
                "alias": {
                    "test": "./src/test",
                    "app": "./src/app",
                    "models": "./src/models",
                    "utils": "./src/utils"
                }
            }
        ]
    ]
}
